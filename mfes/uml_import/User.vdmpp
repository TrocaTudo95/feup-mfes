class User
types 
	public String = seq of char;
			public Date :: day:nat
									 month:nat
									 year:nat;
	
	
instance variables	
  Comments:set of (Comment) := {};
  Posts:set of (Post) := {};
  Friends: set of(User) := {};
  StarFriends : set of(User) := {};
  Family : set of(User) := {};
  BloquedUsers : set of(User) := {};
  
  Birthday:Date;
  Email:String;
  Name:String;
  
operations
 	public User(name: String, birthday: Date, email: String) user: User ==
	(	Name := name; 
		Birthday := birthday;
		Email := email;
		return self) 
	post Comments = {} and Posts = {} and Friends ={} and Name = name; 
	
	public getName() n : String ==
	return Name;
	
	public getBirthday() d: Date ==
	return Birthday;
	
	public getEmail() e : String ==
	return Email;
	
	public addPost(p:Post) ==
  Posts := {p} union Posts
 	post Posts = {p} union Posts~;
 	
 	public addFriend(f:User) ==
 	Friends := {f} union Friends
 	post Friends = {f} union Friends~;
 	
 	public removeFriend(f:User) ==
 	Friends := Friends \{f}
 	post Friends =Friends~ \{f};
	
	
end User
