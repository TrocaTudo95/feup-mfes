class Post
types 
	public String = seq of char;
	
instance variables
  Comments:set of (Comment) := {};
  Content:String;
  
  operations
 	public Post(content: String) p: Post ==
	(	Content := content; return self);
	
	public addComment(c:Comment) ==
  Comments := {c} union Comments
 	post Comments = {c} union Comments~;

end Post
